program sort(input,output);
var a:integer;
var b:real;

var c :array [1..10] of integer;
var d :array [1..10] of real;

begin
    if(a) then write(d) else write(c);
    if(b) then write(a) else write(d);
    if(c) then write(b) else write(a);
    if(d) then write(c) else write(b);
    if(a) then write(d) else write(c);
    if(b) then write(a) else write(d);
    if(c) then write(b) else write(a);

    if(0) then write(d) else write(1);
    if(4) then write(2.2) else write(d);
    if(c) then write(1) else write(4.3)
end.
